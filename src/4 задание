public class Main {
    public static void main(String[] args) {
        byte pro = 16/8;
        short t1= 20;
        short t2 = 60*24;
        int t3 = t2*3;
        int t4 = t3*30;
        int ammount1 = pro*t1;
        int ammount2 = pro*t2;
        int ammount3 = pro*t3;
        int ammount4 = pro*t4;
        System.out.println("За "+t1+" минут работы машины, она произвела "+ammount1+" штук бутылок");
        System.out.println("За сутки работы машины, она произвела "+ammount2+" штук бутылок");
        System.out.println("За 3 суток работы машины, она произвела "+ammount3+" штук бутылок");
        System.out.println("За месяц работы машины, она произвела "+ammount4+" штук бутылок");
    }
}