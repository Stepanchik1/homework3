public class Main {
    public static void main(String[] args) {
        int milk = 200;
        int banana = 5;
        int eggs = 4;
        int icecream = 2;
        var WeightMilk= 105/100;
        var WeightBanana= 80;
        var WeightEgg = 70;
        var WeightIcecrean = 100;
        var totalWeight = milk*WeightMilk+banana*WeightBanana+icecream*WeightBanana+eggs*WeightEgg;
        double weightKg = totalWeight/(double)1000;
        System.out.println("Вес спортивного завтрака составит "+totalWeight+" грамм");
        System.out.println("Вес спортивного завтрака составит "+weightKg+" кг");
    }
}